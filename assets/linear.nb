Cell[BoxData[
 RowBox[{"A", " ", "=", " ", 
  RowBox[{"Array", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"RandomInteger", "[", "10", "]"}], "&"}], ",", " ", 
