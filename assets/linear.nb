    RowBox[{"{", 
     RowBox[{"0", ",", " ", 
      RowBox[{"-", "2"}], ",", " ", "2"}], "}"}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"5", ",", " ", "2", ",", " ", 
      RowBox[{"-", "1"}]}], "}"}]}], "}"}]}]], "Input",
