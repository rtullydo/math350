 CellLabel->
  "In[125]:=",ExpressionUUID->"c978cc75-d265-426b-baf5-4f9f9e54db19"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"0", ",", "10", ",", "10", ",", "5", ",", "7", ",", "3"}], "}"}], 
   ",", 
   RowBox[{"{", 
    RowBox[{"9", ",", "9", ",", "0", ",", "3", ",", "3", ",", "0"}], "}"}], 
