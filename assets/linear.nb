   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"-", "2"}], ",", " ", 
       RowBox[{"-", "3"}], ",", " ", "3", ",", " ", 
       RowBox[{"-", "1"}]}], "}"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", 
       RowBox[{"-", "2"}], ",", " ", "2", ",", " ", 
       RowBox[{"-", "2"}]}], "}"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"5", ",", " ", "2", ",", " ", 
       RowBox[{"-", "1"}], ",", " ", "6"}], "}"}]}], "}"}]}], 
  "\[IndentingNewLine]"}]], "Input",
